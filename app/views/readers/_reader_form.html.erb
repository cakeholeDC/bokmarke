<%= form_for @reader do |f| %>
	<div>
		<%= f.label :username %>
		<%= f.text_field :username %>
	</div>
	<div>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>
	<div>
		<%= f.label :age %>
		<%= f.number_field :age %>
	</div>
	<div>
		<%= f.label :bio %>
		<%= f.text_area :bio %>
	</div>
	<div>
		<%= f.submit submit_text %>
	</div>
<% end %>